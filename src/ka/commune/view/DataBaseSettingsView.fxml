<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXProgressBar?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<ScrollPane fitToHeight="true" fitToWidth="true" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ka.commune.view.DataBaseSettingsController">
    <content>
        <HBox alignment="CENTER">
            <children>
                <Pane prefHeight="400.0" prefWidth="100.0" HBox.hgrow="ALWAYS" />
                <VBox alignment="TOP_CENTER" prefHeight="400.0" prefWidth="443.0">
                    <children>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="30.0">
                            <children>
                                <Button contentDisplay="RIGHT" graphicTextGap="8.0" mnemonicParsing="false" onAction="#importDataBase" prefHeight="44.0" prefWidth="188.0" style="-fx-background-color: #0984e3;" text="تحميل قاعدة البيانات" textFill="WHITE">
                                    <font>
                                        <Font name="System Bold" size="15.0" />
                                    </font>
                           <graphic>
                              <FontAwesomeIconView fill="WHITE" glyphName="DOWNLOAD" size="20" />
                           </graphic>
                                </Button>
                        <Button contentDisplay="RIGHT" graphicTextGap="8.0" mnemonicParsing="false" onAction="#exportDataBase" prefHeight="44.0" prefWidth="170.0" style="-fx-background-color: #10ac84;" text="حفظ قاعدة البيانات" textFill="WHITE">
                           <font>
                              <Font name="System Bold" size="15.0" />
                           </font>
                           <graphic>
                              <FontAwesomeIconView fill="WHITE" glyphName="SAVE" size="20" />
                           </graphic>
                        </Button>
                            </children>
                        </HBox>
                  <HBox alignment="CENTER" prefHeight="157.0" prefWidth="443.0">
                     <children>
                        <JFXProgressBar fx:id="progressBar" maxWidth="1.7976931348623157E308" prefHeight="44.0" secondaryProgress="1.0" HBox.hgrow="ALWAYS" />
                     </children>
                  </HBox>
                    </children>
               <padding>
                  <Insets top="30.0" />
               </padding>
                </VBox>
                <Pane prefHeight="400.0" prefWidth="100.0" HBox.hgrow="ALWAYS" />
            </children>
        </HBox>
    </content>
</ScrollPane>
